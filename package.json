{
  "private": true,
  "name": "alexsinfarosa.com",
  "description": "Alex Sinfarosa Website",
  "license": "",
  "scripts": {
    "clean": "rimraf .cache ./build ./public/build",
    "build": "cross-env NODE_ENV=production npm run build:css remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "postinstall": "remix setup cloudflare-pages",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --watch ./build",
    "start": "npm run dev:wrangler",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify"
  },
  "dependencies": {
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.0",
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.2",
    "cross-env": "^7.0.3",
    "esbuild": "0.14.11",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.5.4",
    "wrangler": "^0.0.7"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
